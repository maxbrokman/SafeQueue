{
    "name": "zmaglica/safe-queue",
    "description": "A Laravel Doctrine friendly daemonising queue worker for Laravel 5",
    "license": "MIT",
    "authors": [
        {
            "name": "Zvonimir Maglica",
            "email": "zvonimirmaglica4@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/queue": "^5.4.9",
        "laravel-doctrine/orm": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "mockery/mockery": "^0.9.4",
        "fabpot/php-cs-fixer": "^1.11",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Zmaglica\\SafeQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\Zmaglica\\SafeQueue\\": "tests/"
        }
    }
}
